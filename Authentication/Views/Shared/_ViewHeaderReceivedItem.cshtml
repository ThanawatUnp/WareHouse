@foreach (var item in ViewBag.HeaderReceive)
{
    @switch (@item)
    {
        case "Cost":
            <th style="min-width: 110px;">@item</th>
            break;
        case "State":
        case "Expire Date":
        case "Receive Date":
        case "Create By":
        case "Create Date":
        case "Edit By":
        case "Edit Date":
            <th style="min-width: 130px;">@item</th>
            break;
        case "Receive Quantity":
            <th style="min-width: 150px;">@item</th>
            break;
        case "Order Number":
        case "Lot Number":
            <th style="min-width: 160px;">@item</th>
            break;
        case "Item Detail":
            <th style="min-width: 180px;">@item</th>
            break;
        case "Description":
            <th style="min-width: 230px;">@item</th>
            break;
        default:
            <th>@item</th>
            break;
    }
}

