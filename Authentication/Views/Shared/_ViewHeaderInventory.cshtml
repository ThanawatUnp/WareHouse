@foreach (var item in ViewBag.Header)
{
    @switch (@item)
    {
        case "Quantity":
            <th style="min-width: 90px;">@item</th>
            break;
        @*case "Cost":
            <th style="min-width: 110px;">@item</th>
            break;*@
        case "Item Category":
            <th style="min-width: 140px;">@item</th>
            break;
        case "Item Code":
            <th style="min-width: 150px;">@item</th>
            break;
        case "Item Name":
            <th style="min-width: 160px;">@item</th>
            break;
        default:
            <th>@item</th>
            break;
    }
}