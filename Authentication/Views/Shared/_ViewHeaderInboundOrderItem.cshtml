@foreach (var item in ViewBag.HeaderSubMenu)
{
    @switch (@item)
    {
        case "Cost":
            <th style="min-width: 110px;">@item</th>
            break;
        case "Item Status":
        case "Create By":
        case "Create Date":
        case "Edit By":
        case "Edit Date":
            <th style="min-width: 130px;">@item</th>
            break;
        case "Quantity Order":
        case "Remain Quantity":
            <th style="min-width: 150px;">@item</th>
            break;
        case "Line Number":
        case "Item Type":
            <th style="min-width: 160px;">@item</th>
            break;
        case "Item Name":
            <th style="min-width: 180px;">@item</th>
            break;
        default:
            <th>@item</th>
            break;
    }
}
