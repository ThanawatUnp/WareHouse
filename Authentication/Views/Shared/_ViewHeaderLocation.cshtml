@foreach (var item in ViewBag.Header)
{
    @switch (@item)
    {
       @*  case "Mix Item":
        case "Mix Lot":
            <th style="min-width: 90px;">@item</th>
            break;
        case "Mix Expire":
            <th style="min-width: 100px;">@item</th>
            break; *@
        case "Create By":
        case "Create Date":
        case "Edit By":
        case "Edit Date":
            <th style="min-width: 130px;">@item</th>
            break;
        case "Location Code":
        case "Description":
            <th style="min-width: 150px;">@item</th>
            break;
        case "Location Category":
            <th style="min-width: 180px;">@item</th>
            break;
        default:
            <th>@item</th>
            break;
    }
}